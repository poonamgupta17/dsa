//to find the floor(largest no in array<=x) of a given element x in a sorted array
//tc = O(logn)
//sc= O(1)
#include <bits/stdc++.h>
using namespace std;    
class Solution {
  public:
    int findFloor(vector<int>& arr, int x) {
        // code here
        int n=arr.size();
        int low=0;
        int high=n-1;
        int ans=-1;
        while(low<=high){
            int mid=(low+high)/2;
            if(arr[mid]<=x){
                ans=arr[mid];
                low=mid+1;
            }
            else{
                high=mid-1;
            }
        }
        return ans;
    }
};
