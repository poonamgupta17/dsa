//to count number of subarrays with given xor k
//tc=o(n)
//sc=o(n)
#include <bits/stdc++.h>
using namespace std;
class Solution {
  public:
    long subarrayXor(vector<int> &arr, int k) {
        // code here
        int n=arr.size();
        int cnt=0;
        int xo=0;
        map<int,int>mpp;
        mpp[xo]++;
        for(int i=0;i<n;i++){
            xo=xo^arr[i];
            int x=xo^k;
            cnt+=mpp[x];
            mpp[xo]++;
        }
        return cnt;
    }
};